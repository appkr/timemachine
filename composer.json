{
    "name": "appkr/timemachine",
    "description": "Laravel/Lumen application-level time setter for testing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "appkr",
            "email": "juwonkim@me.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.0,<5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*|5.*|6.*",
        "mockery/mockery": "0.9.*",
        "laravel/laravel": "^5.0,<5.6"
    },
    "autoload": {
        "psr-4": {
            "Appkr\\Timemachine\\": "src/Timemachine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Appkr\\Timemachine\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "\\Appkr\\Timemachine\\TimemachineServiceProvider"
            ]
        }
    }
}
